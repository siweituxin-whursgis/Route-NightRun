<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.huyigong.route_nightrun">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="夜跑小助手"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">

        <activity
            android:name=".WeatherActivity"
            android:label="夜跑小助手" />

        <activity
            android:name=".Night_Running"
            android:label="Night Running" />

        <activity
            android:name=".RunningActivity"
            android:label="Running" />

        <activity android:name=".NightRunActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action." /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".run_competition" />

        <activity android:name=".Calculate_KM" />

        <activity android:name=".enroll_marathon" />

        <activity android:name=".success_submit" />

        <activity android:name=".weight_management" />

        <activity android:name=".IndoorRunActivity" />

        <activity
            android:name=".AlertActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_alert"
            android:theme="@style/FullscreenTheme" />

        <receiver
            android:name=".AlarmReceiver"
            android:process=":remote" />

        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="夜跑小助手">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".GymInfoActivity" />

        <activity android:name=".Call_Taxi" />

        <activity android:name=".taxi_coming" />

        <activity android:name=".SearchNearPeopleActivity" />

        <activity
            android:name=".Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".Register_new"
            android:label="@string/title_activity_register_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Register_choose" />

        <activity
            android:name=".OutMenRegister"
            android:label="@string/title_activity_out_men_register"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".Before_Login"
            android:label="夜跑小助手"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".forum"
            android:label="跑友秀" />

        <activity android:name=".MyChatActivity" />

        <activity
            android:name=".ShuoshuoActivity"
            android:label="夜跑论坛"
            android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>